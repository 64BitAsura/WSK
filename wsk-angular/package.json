{
  "name": "wsk-angular",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "clean": "--glob .*/**/node_modules .*/**/package-lock.json",
    "package": "@angular/cli",
    "version": "@latest",
    "version_current": "@latest",
    "version_legacy": "@12.0.0",
    "version_future": "@next",
    "template": "scss",
    "project": "wsk-app",
    "prefix": "my",
    "component": "component"
  },
  "scripts": {
    "preclean": "npx -y rimraf $npm_package_config_project",
    "clean": "npx -y rimraf $npm_package_config_clean",
    
    "pre-init": "npx -y $npm_package_config_package$npm_package_config_version new $npm_package_config_project --style=$npm_package_config_template --routing --interactive=false --defaults --skip-install",
    "preinit": "npx -y -p $npm_package_config_package$npm_package_config_version ng new $npm_package_config_project --style=$npm_package_config_template --routing --skip-install",
    "_init": "cd $npm_package_config_project && npm link stencil-library && npm link angular-workspace && npm link angular-library",
    "init": "cd $npm_package_config_project && npm link stencil-library angular-library",
    "postinit": "node .bin/index install && npm run setup",
    
    "prestart": "npm run clean",
    "start": "npm run init",
    "poststart": "",

    "prestart:legacy": "npx -y rimraf $npm_package_config_project-legacy",
    "start:legacy": "npx -y -p $npm_package_config_package$npm_package_config_version_legacy ng new $npm_package_config_project-legacy --style=$npm_package_config_template --routing --skip-install",
    "poststart:legacy": "npm run setup:legacy",
    "pre-setup:legacy": "cd $npm_package_config_project-legacy && npm i --legacy-peer-deps && npm link stencil-library && npm link angular-library",
    "presetup:legacy": "cd $npm_package_config_project-legacy && npm link stencil-library angular-library",
    "setup:legacy": "node .bin/index install legacy",
    "post-setup:legacy": "npx -y ncp ./.tmp/src/app ./$npm_package_config_project-legacy/src/app && cd $npm_package_config_project-legacy && npm i --force or --legacy-peer-deps",

    "prestart:future": "npx -y rimraf $npm_package_config_project-future",
    "start:future": "npx -y -p $npm_package_config_package$npm_package_config_version_future ng new $npm_package_config_project-future --style=$npm_package_config_template --routing --skip-install",
    "poststart:future": "npm run setup:future",
    "presetup:future": "cd $npm_package_config_project-future && npm link stencil-library angular-library",
    "setup:future": "node .bin/index install future",
    "post-setup:future": "npx -y ncp ./.tmp/src/app ./$npm_package_config_project-future/src/app && cd $npm_package_config_project-legacy && npm i --force or --legacy-peer-deps",
    
    "presetup": "echo \"Warning: may need to update this script from time to time\"",
    "setup": "npx -y ncp ./.tmp/src/app ./$npm_package_config_project/src/app",
    "postsetup": "cd $npm_package_config_project && npm run build && npm run start",

    "links": "npm ls -g --depth=0 --link=true",
    "view": "npm view $npm_package_config_package versions --json",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
