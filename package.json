{
  "name": "wsk",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "clean": "--glob .*/**/node_modules .*/**/package-lock.json"
  },
  "scripts": {
    "pre-start": "npm run clean",
    "start": "npx -y npm-run-all stencil:start angular:start",
    "post-start": "npx -y npm-run-all --parallel angular:start react:start",
    "preclean": "npx -y npm-run-all --parallel angular:clean react:clean stencil:clean",
    "clean": "npx -y rimraf $npm_package_config_clean",
    "postclean": "",
    "angular:setup": "cd wsk-angular && npm run setup",
    "angular:postsetup": "cd wsk-angular && npm run postsetup",
    "angular:start": "cd wsk-angular && npm run start",
    "angular:wsk:start": "cd wsk-angular/wsk-app && npm run start",
    "angular:clean": "cd wsk-angular && npm run clean",
    "javascript:setup": "cd wsk-javascript && npm run setup",
    "javascript:start": "cd wsk-javascript && npm run start",
    "javascript:clean": "cd wsk-javascript && npm run clean",
    "react:setup": "cd wsk-react && npm run setup",
    "react:start": "cd wsk-react && npm run start",
    "react:clean": "cd wsk-react && npm run clean",
    "stencil:setup": "cd wsk-stencil && npm run setup",
    "stencil:start": "cd wsk-stencil && npm run start",
    "stencil:clean": "cd wsk-stencil && npm run clean",
    "stencil:uninstall": "cd wsk-stencil && npm run uninstall:all",
    "typescript:setup": "cd wsk-typescript && npm run setup",
    "typescript:start": "cd wsk-typescript && npm run start",
    "typescript:clen": "cd wsk-typescript && npm run clen",
    "vue:setup": "cd wsk-vue && npm run setup",
    "vue:start": "cd wsk-vue && npm run start",
    "vue:clean": "cd wsk-vue && npm run clean",
    "presetup": "npm i prompts yargs",
    "setup": "npx -y npm-run-all --parallel angular:setup react:setup stencil:setup",
    "postsetup": "",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "prompts": "^2.4.2",
    "yargs": "^17.7.2"
  }
}
