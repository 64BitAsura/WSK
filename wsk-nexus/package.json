{
  "name": "nexus",
  "version": "2.1.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "url": ""
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "playbook/*",
      "testautomation"
    ],
    "nohoist": [
      "**/css-loader",
      "**/webpack-dev-server"
    ]
  },
  "watch": {
    "build:playbook": {
      "patterns": [
        "playbook",
        "packages/nexus"
      ],
      "extensions": "hbs,less,md",
      "quiet": false
    },
    "build:stencil:examples": {
      "patterns": [
        "playbook/stencil"
      ],
      "extensions": "html, js",
      "quiet": false
    },
    "update:styles": {
      "patterns": [
        "packages/nexus/src/styles"
      ],
      "extensions": "scss",
      "quiet": false
    }
  },
  "scripts": {
    "prepare": "husky install",
    "restart": "yarn run clean && yarn install && yarn run build",
    "pipeline": "yarn install && yarn build && yarn lint && yarn test && yarn test:pa11y:ci && yarn build:playbook && yarn build:sb",
    "preinstall": "node bin/engines",
    "clean": "node bin/clean",
    "_build": "rimraf target && yarn run build:lint && yarn run build:test && yarn run build:nexus && yarn run build:playbook:docs",
    "prebuild": "yarn run build:test",
    "build": "rimraf target && yarn run build:nexus && yarn run build:playbook:docs",
    "build-lint": "yarn run lint && yarn run lint:fix && yarn run prettier:format",
    "build:lint": "yarn run lint",
    "prebuild:test": "yarn run lint",
    "build:test": "yarn run test",
    "postbuild:test": "yarn run test:pa11y:ci",
    "build:storybook": "node bin/sb/index.js && yarn run build:sb",
    "build:nexus": "yarn run build:stencil && yarn run build:stencil:angular && yarn run build:stencil:react",
    "build:playbook:docs": "yarn run build:angular && yarn run build:react && yarn run build:playbook:icons && yarn run build:playbook && yarn run copy:config",
    "build:stencil": "yarn run build:stencil:examples && yarn --cwd packages/nexus run build",
    "build:stencil:angular": "yarn --cwd packages/nexus-angular run build",
    "build:stencil:react": "yarn --cwd packages/nexus-react run build",
    "build:angular": "yarn --cwd playbook/angular run build",
    "build:react": "yarn --cwd playbook/react run build",
    "build:playbook:icons": "node bin/documentIcons",
    "build:playbook": "node bin/documentCssVars && playbook && yarn run copy:playbook:assets",
    "build:stencil:examples": "playbook-plugin-html",
    "build:sb:stencil": "yarn --cwd playbook/stencil run build-sb",
    "build:sb:react": "yarn --cwd playbook/react run build-sb",
    "build:sb:angular": "yarn --cwd playbook/angular run build-sb",
    "prebuild:sb": "node bin/updateSBTS.js && yarn run setup:env:prod && yarn run sb:docs",
    "build:sb": "yarn run build:sb:stencil && yarn run build:sb:react && yarn run build:sb:angular",
    "postbuild:sb": "node bin/sbdeploy.js",
    "copy:config": "ncp ./web.config ./target/www/playbook/web.config",
    "copy:playbook:assets": "mkdirp target/www/assets/fonts && mkdirp target/www/assets/css && mkdirp target/www/playbook/assets/css/assets && ncp packages/nexus/src/assets target/www/playbook/assets/css/assets && ncp packages/nexus/src/assets target/www/playbook/react/assets && ncp packages/nexus/dist/nexus/nexus.css target/www/playbook/assets/css/nexus.css",
    "docker:ci": "docker run -it --rm -v ${PWD}:/srv/ docker.ey-intuitive.com/gitlab-puppeteer bash",
    "lint": "yarn run lint:stencil && yarn run lint:angular && yarn run lint:react && yarn run lint:scss && yarn run lint:bin",
    "lint:angular": "yarn --cwd playbook/angular run lint",
    "lint:react": "yarn --cwd playbook/react run lint",
    "lint:testautomation": "yarn --cwd testautomation run lint",
    "lint:bin": "eslint ./bin",
    "lint:scss": "stylelint \"./**/*.scss\"",
    "lint:stencil": "yarn --cwd packages/nexus run lint",
    "hook": "yarn run lint:fix && yarn run test && yarn run test:pa11y:ci && yarn run prettier:format",
    "lint:fix": "yarn run prettier:format && yarn run lint:stencil:fix && yarn run lint:angular:fix && yarn run lint:react:fix && yarn run lint:scss:fix && yarn run lint:bin:fix",
    "prettier:format": "yarn run prettier:format:js && yarn run prettier:format:ts && yarn run prettier:format:tsx && yarn run prettier:format:html && yarn run prettier:format:scss",
    "prettier:format:js": "prettier --eslint-config-path packages/nexus/.eslintrc.js --write '**/*.js'",
    "prettier:format:ts": "prettier --eslint-config-path packages/nexus/.eslintrc.js --write '**/*.ts'",
    "prettier:format:tsx": "prettier --eslint-config-path packages/nexus/.eslintrc.js --write '**/*.tsx'",
    "prettier:format:html": "prettier --eslint-config-path packages/nexus/.eslintrc.js --write '**/*.html'",
    "prettier:format:scss": "prettier --eslint-config-path packages/nexus/.eslintrc.js --write '**/*.scss'",
    "lint:angular:fix": "yarn --cwd playbook/angular run lint --fix",
    "lint:react:fix": "yarn --cwd playbook/react run lint --fix",
    "lint:bin:fix": "eslint --fix ./bin",
    "lint:scss:fix": "stylelint \"./**/*.scss\" --fix",
    "lint:stencil:fix": "yarn --cwd packages/nexus run lint --fix",
    "update:styles": "touch packages/nexus/src/styles.scss",
    "renamer": "renamer",
    "start": "concurrently \"yarn run watch\" \"yarn run start:angular\" \"yarn run start:stencil\" \"yarn run start:react\" \"yarn run serve:playbook\" \"yarn run start:sb\"",
    "start:angular": "yarn --cwd playbook/angular run start --open=true",
    "start:stencil": "yarn --cwd packages/nexus run start",
    "start:react": "yarn --cwd playbook/react run start",
    "sb:documentColors": "node bin/documentColors",
    "sb:documentIcons": "node bin/sbDocumentIcons",
    "sb:documentTheme": "node bin/documentTheme",
    "sb:docs": "yarn run sb:documentTheme && yarn run sb:documentColors && yarn run sb:documentIcons",
    "setup:env": "ncp .env-example ./playbook/stencil/.env && ncp .env-example ./playbook/angular/.env && ncp .env-example ./playbook/react/.env",
    "setup:env:prod": "ncp .env-example-prod ./playbook/stencil/.env && ncp .env-example-prod ./playbook/angular/.env && ncp .env-example-prod ./playbook/react/.env",
    "start:sb": "yarn run setup:env && yarn run sb:docs && concurrently \"yarn run start:sb:stencil\" \"yarn run start:sb:react\" \"yarn run start:sb:angular\"",
    "start:sb:stencil": "yarn --cwd playbook/stencil run sb",
    "start:sb:react": "yarn --cwd playbook/react run sb",
    "start:sb:angular": "yarn --cwd playbook/angular run sb",
    "start:ie": "concurrently \"yarn run watch\" \"yarn run start:angular\" \"yarn run start:stencil --es5\" \"yarn run start:react\" \"yarn run serve:playbook\"",
    "start:stencil:ie": "yarn run start:stencil --es5",
    "serve:playbook": "node playbook/server.js",
    "serve:playbook:ci": "node playbook/server.ci.js",
    "cy:test": "yarn --cwd testautomation run test",
    "cy:open:test": "yarn --cwd testautomation run cy:open",
    "cy:run:vrt": "yarn --cwd testautomation run cy:run:vrt",
    "cy:run:cyt": "yarn --cwd testautomation run cy:run:cyt",
    "test": "yarn run test:stencil",
    "test:pa11y": "node bin/pa11y",
    "test:pa11y:ci:port": "node bin/pa11y 3301",
    "test:pa11y:ci": "start-server-and-test serve:playbook:ci http-get://localhost:3301/playbook test:pa11y:ci:port",
    "test:stencil:watch": "yarn --cwd packages/nexus run test.watch",
    "test:stencil": "yarn --cwd packages/nexus run test",
    "test:watch": "yarn run test:stencil:watch",
    "watch": "npm-watch",
    "scan": "npm audit --production",
    "prerelease": "yarn run scan",
    "release": "echo \"TODO: take in the release scope and call the node release command\" && node bin/release $npm_release",
    "release:patch": "node bin/release patch",
    "release:minor": "node bin/release minor",
    "release:major": "node bin/release major"
  },
  "bin": {
    "nexus": "bin/cli.js"
  },
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=16.14"
  },
  "engineStrict": true,
  "author": "EY Nexus",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/eslint-parser": "7.16.5",
    "@ey-studio-phl/playbook": "1.4.1",
    "@ey-studio-phl/playbook-plugin-html": "1.0.6",
    "@panosvoudouris/addon-versions": "1.0.1",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-backgrounds": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-jest": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/preset-scss": "1.0.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "6.5.16",
    "@types/vfile-message": "2.0.0",
    "@typescript-eslint/eslint-plugin": " 5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "async": "3.1.0",
    "change-case": "4.1.2",
    "codelyzer": "6.0.2",
    "concurrently": "6.5.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "express": "4.17.1",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest-junit": "13.0.0",
    "jest-junit-reporter": "1.1.0",
    "lint-staged": "12.3.6",
    "mkdirp": "1.0.3",
    "moment": "2.29.4",
    "ncp": "2.0.0",
    "npm-watch": "0.11.0",
    "open": "7.0.0",
    "pa11y": "6.1.1",
    "prettier": "2.8.8",
    "prismjs": "1.27.0",
    "puppeteer": "14.1.1",
    "raw-loader": "4.0.2",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.4",
    "remark-code-import": "0.2.0",
    "renamer": "4.0.0",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "sass": "1.49.9",
    "sass-loader": "12.4.0",
    "start-server-and-test": "1.14.0",
    "stencil-inline-svg": "1.0.1",
    "storybook-addon-designs": "6.3.1",
    "storybook-addon-pseudo-states": "2.1.1",
    "storybook-addon-turbo-build": "1.1.0",
    "style-loader": "3.3.1",
    "stylelint": "14.6.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-scss": "^4.0.0",
    "ts-jest": "27.1.2",
    "tslint": "6.1.3",
    "tslint-stencil": "1.0.1",
    "yargs": "17.3.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.16.7",
    "lodash": "4.17.21",
    "memoize-one": "^6.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css,scss}": "prettier --write"
  }
}
