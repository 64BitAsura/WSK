{
  "name": "@nexus/react",
  "sideEffects": false,
  "version": "2.1.0-20231102.9",
  "description": "React specific wrapper for nexus",
  "scripts": {
    "build": "npm run clean && npm run compile && npx -y ncp package.json dist/package.json",
    "clean": "npx -y rimraf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p ."
  },
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "react": "*",
    "react-dom": "*",
    "typescript": "5.1.6",
    "@types/react": "*",
    "@types/react-dom": "*"
  }
}
